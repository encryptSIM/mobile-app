{
  "name": "encrypt-sim",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "gen:schema": "openapi-typescript ./api/api.yaml -o ./api/schema.ts",
    "gen-airalo:schema": "openapi-typescript ./airalo-api/airalo-api.json -o ./airalo-api/schema.ts",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:prev": "infisical run --env staging --path='/mobile' -- eas build --platform android --profile preview --local --output './preview.apk'",
    "build:dev": "infisical run --env dev --path='/mobile' -- eas build --platform android --profile development --local --output './development.apk'",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@solana-mobile/mobile-wallet-adapter-protocol",
          "@solana-mobile/mobile-wallet-adapter-protocol-web3js",
          "@solana/spl-token",
          "@solana/web3.js",
          "@wallet-standard/core",
          "deepmerge",
          "js-base64"
        ]
      }
    }
  },
  "dependencies": {
    "@expo/config": "^11.0.0",
    "@expo/plist": "^0.3.5",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-clipboard/clipboard": "^1.16.2",
    "@react-native-picker/picker": "2.11.1",
    "@react-native-vector-icons/fontawesome": "^12.2.0",
    "@react-navigation/native": "^7.0.14",
    "@solana-mobile/mobile-wallet-adapter-protocol": "^2.2.2",
    "@solana-mobile/mobile-wallet-adapter-protocol-web3js": "^2.2.2",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.2",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.9.0",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "ed25519-hd-key": "^1.3.0",
    "ethers": "^6.14.0",
    "expo": "53.0.19",
    "expo-build-properties": "^0.14.8",
    "expo-clipboard": "~7.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-device": "^7.1.4",
    "expo-file-system": "^18.1.11",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.3",
    "expo-secure-store": "~14.2.3",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-updates": "~0.28.17",
    "expo-web-browser": "~14.2.0",
    "framer-motion": "^12.23.7",
    "fuse.js": "^7.1.0",
    "nativewind": "^4.1.23",
    "openapi-fetch": "^0.14.0",
    "openapi-react-query": "^0.5.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-country-flag": "^2.0.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-paper": "^5.14.5",
