{
  "expo": {
    "name": "encryptSIM",
    "slug": "encryptsim",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "encryptsim",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0F1C"
    },
    "ios": {
      "supportsTablet": true,
      "deploymentTarget": "15.1",
      "splash": {
        "image": "./assets/splash/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#0A0F1C"
      },
      "bundleIdentifier": "com.encryptsim.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": [
          "https",
          "solana-wallet",
          "phantom",
          "solflare",
          "glow",
          "trust",
          "backpack",
          "mathwallet",
          "metamask"
        ]
      }
    },
    "android": {
      "splash": {
        "image": "./assets/splash/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#0A0F1C"
      },
      "package": "com.encryptsim.app",
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "minSdkVersion": 23,
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ],
          "data": {
            "scheme": "https",
            "host": "encryptsim.com"
          }
        },
        {
          "action": "VIEW",
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ],
          "data": {
            "scheme": "encryptSIM"
          }
        }
      ],
      "adaptiveIcon": {
        "backgroundColor": "#32D583",
        "foregroundImage": "./assets/android-forground-icon.png"
      }
    },
    "web": {
      "favicon": "./public/favicon.ico",
      "name": "encryptSIM",
      "shortName": "encryptSIM",
      "description": "Your Web3 gateway. No KYC, crypto-ready, instant eSIM for secure global use.",
      "themeColor": "#F8FAFC",
      "backgroundColor": "#F8FAFC",
      "display": "standalone",
      "orientation": "portrait",
      "lang": "en",
      "meta": {
        "apple-mobile-web-app-capable": "yes",
        "apple-mobile-web-app-status-bar-style": "black-translucent",
        "apple-mobile-web-app-title": "encryptSIM",
        "application-name": "encryptSIM",
        "msapplication-TileColor": "#F8FAFC",
        "msapplication-tap-highlight": "no",
        "mobile-web-app-capable": "yes"
      }
    },
    "plugins": [
      "expo-secure-store",
      "expo-router",
      [
        "expo-build-properties"
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "390debb1-f0a1-447b-b8e5-b78acb7da275"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/504c0721-f7ed-48de-8f62-79c5efaf7a4c"
    },
    "owner": "encryptsim"
  }
}
