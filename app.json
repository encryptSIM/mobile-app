{
  "expo": {
    "name": "encryptSIM",
    "slug": "encryptsim",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-logo.png",
    "resizeMode": "contain",
    "scheme": "encryptSIM",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0F1C"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#0A0F1C"
      },
      "bundleIdentifier": "com.giachan2002.encryptsim",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "splash": {
        "image": "./assets/splash/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#0A0F1C"
      },
      "package": "com.giachan2002.encryptsim"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f311d411-62b2-4dec-b531-1fd384fd0a64"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/504c0721-f7ed-48de-8f62-79c5efaf7a4c"
    },
    "owner": "jack.kitto"
  }
}
