{
  "expo": {
    "name": "encryptSIM",
    "slug": "encryptsim",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-logo.png",
    "resizeMode": "contain",
    "scheme": "encryptSIM",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0F1C"
    },
    "ios": {
      "supportsTablet": true,
      "deploymentTarget": "15.1",
      "splash": {
        "image": "./assets/splash/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#0A0F1C"
      },
      "bundleIdentifier": "com.giachan2002.encryptsim",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": [
          "https",
          "solana-wallet",
          "phantom",
          "solflare",
          "glow",
          "trust",
          "backpack",
          "mathwallet",
          "metamask"
        ]
      }
    },
    "android": {
      "splash": {
        "image": "./assets/splash/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#0A0F1C"
      },
      "package": "com.giachan2002.encryptsim",
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "minSdkVersion": 23,
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ],
          "data": {
            "scheme": "https",
            "host": "encryptsim.com"
          }
        },
        {
          "action": "VIEW",
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ],
          "data": {
            "scheme": "encryptSIM"
          }
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 23
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f311d411-62b2-4dec-b531-1fd384fd0a64"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/504c0721-f7ed-48de-8f62-79c5efaf7a4c"
    },
    "owner": "jack.kitto"
  }
}
