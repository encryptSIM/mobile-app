{
  "cli": {
    "version": ">= 16.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ENVIRONMENT": "development",
        "EXPO_PUBLIC_ENVIRONMENT": "development",
        "EXPO_PUBLIC_RPC_URL": "https://api.testnet.solana.com",
        "EXPO_PUBLIC_API_URL": "https://api-63k4aibnea-de.a.run.app"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "ENVIRONMENT": "preview",
        "EXPO_PUBLIC_ENVIRONMENT": "preview",
        "EXPO_PUBLIC_RPC_URL": "https://api.testnet.solana.com",
        "EXPO_PUBLIC_API_URL": "https://api-63k4aibnea-de.a.run.app"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "prod",
        "EXPO_PUBLIC_RPC_URL": "https://api.mainnet-beta.solana.com",
        "EXPO_PUBLIC_API_URL": "https://backend-80282010584.asia-southeast1.run.app"
      },
      "android": {
        "ndk": "26.2.11394342",
        "image": "latest",
        "buildType": "app-bundle"
      }
    },
    "dapp-store": {
      "channel": "development",
      "android": {
        "buildType": "apk",
        "ndk": "26.2.11394342",
        "image": "latest"
      },
      "config": "dapp-store.yml"
    }
  },
  "submit": {
    "production": {}
  }
}
