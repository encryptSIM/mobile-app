{
  "cli": {
    "version": ">= 16.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_RPC_URL": "https://api.devnet.solana.com",
        "EXPO_PUBLIC_API_URL": "https://example.com"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_RPC_URL": "https://api.testnet.solana.com",
        "EXPO_PUBLIC_API_URL": "https://example.com"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "ndk": "26.2.11394342",
        "image": "latest",
        "buildType": "app-bundle"
      }
    },
    "dapp-store": {
      "channel": "development",
      "android": {
        "buildType": "apk",
        "ndk": "26.2.11394342",
        "image": "latest"
      },
      "config": "dapp-store.yml"
    }
  },
  "submit": {
    "production": {}
  }
}
