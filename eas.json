{
  "cli": {
    "version": ">= 16.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_RPC_URL": "https://api.devnet.solana.com",
        "EXPO_PUBLIC_API_URL": "https://example.com"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_RPC_URL": "https://api.testnet.solana.com",
        "EXPO_PUBLIC_API_URL": "https://example.com"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_RPC_URL": "https://api.mainnet-beta.solana.com",
        "EXPO_PUBLIC_API_URL": "https://example.com"
      }
    },
    "dapp-store": {
      "channel": "development",
      "android": {
        "buildType": "apk"
      },
      "config": "dapp-store.yml"
    }
  },
  "submit": {
    "production": {}
  }
}
